{
  "arrowParens": "avoid",
  "semi": false,
  "trailingComma": "none",
  "endOfLine": "lf",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 120,
  "jsxSingleQuote": false,
  "no-unused-vars": "off",
  "@typescript-eslint/no-unused-vars": ["error"],
  "includeExport": true,
  "no-duplicate-imports": "off",
  "@typescript-eslint/no-duplicate-imports": ["error"],
  "@typescript-eslint/no-explicit-any": "error"
}
